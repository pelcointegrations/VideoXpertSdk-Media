<package>
  <metadata>
    <id>VideoXpertSdk-Media</id>
    <title>VideoXpert SDK Media Controller</title>
    <version>3.0.0000</version>
    <authors>Pelco by Schneider Electric</authors>
    <owners>Pelco by Schneider Electric</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://raw.githubusercontent.com/pelcointegrations/VideoXpertSdk/master/Nuget/pelco-integrations-64x64.png</iconUrl>
